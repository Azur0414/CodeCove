<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">


<tiles-definitions>

	<definition name="commonTemplate" template="/WEB-INF/views/commonTemplate.jsp">
		<put-attribute name="header" value="/WEB-INF/views/commons/header.jsp" />
	</definition>
	
	<definition name="mainpages/*" extends="commonTemplate">
		<put-attribute name="content" value="/WEB-INF/views/mainpages/content.jsp" />
		<put-attribute name="footer" value="/WEB-INF/views/mainpages/footer.jsp" />
	</definition>
	
	
</tiles-definitions>














