#writeModal {
	position: fixed;
	height: 100%;
    width: 100%;
    left: 0;
    top: 0;
    background-color: rgba(0,0,0,0.5);
    display: none;
}

#writeCont{
	width:60%;
	height: 80%;
	margin-top:10%;
	margin-left: 20% ;
	margin-right: 20% ;
	background-color: white;
	color: black;
}

.form_radio_btn {
	width: 47%;
	height : 45px;
	border: 1px solid #EAE7E7;
	border-radius: 10px;
}
.form_radio_btn input[type=radio] {
	display: none;
}
.form_radio_btn label {
	display: block;
	border-radius: 10px;
	margin: 0 auto;
	text-align: center;
	height: -webkit-fill-available;
	line-height: 45px;
}
 
/* Checked */
.form_radio_btn input[type=radio]:checked + label {
	background: #184DA0;
	color: #fff;
}
 
/* Hover */
.form_radio_btn label:hover {
	color: #666;
}
 
/* Disabled */
.form_radio_btn input[type=radio] + label {
	background: #F9FAFC;
	color: #666;
}
#tagInput {
	font-size: 15px;
	height: 30px;
	padding: 15px;
 	width: 25%; 
 	float:left;
}
.tags {
	background-color: #F2F2F2;
	color: black;
	margin-left: 3px;
	margin-top: 3px;
	border: 5px solid #F2F2F2;
	font-weight: 500;
	border-radius: 10px;
	display: inline-block;
}
</style>
